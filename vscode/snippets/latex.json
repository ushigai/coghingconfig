{
	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
			"table": {
        "prefix": "table",
        "body": [
            "\\begin{table}[!hb]",
            "	\\renewcommand{\\tablename}{表}",
            "	\\begin{center}",
            "		\\caption{tablename}",
            "		\\begin{tabular}{|c|c|} \\hline",
            "			a&b\\\\\\\\\\hline\\hline",
            "			a&b\\\\\\",
            "			a&b\\\\\\",
            "			a&b\\\\\\",
            "			\\hline",
            "		\\end{tabular}",
            "		\\label{}",
            "	\\end{center}",
            "\\end{table}",
        ],
        "description": "表作成"
    },
	"code": {
        "prefix": "code",
        "body": [
            "\\begin{lstlisting}[caption=C言語,label=Clang]",
            "#include<stdio.h>",
            "int main(){",
            "    printf(\"Hello world!\\n\");",
            "}",
            "\\end{lstlisting}",
        ],
        "description": "C言語のソースコードを挿入"
    },
    "figure": {
        "prefix": "figure",
        "body": [
            "\\begin{figure}[H]",
            "	\\begin{center}",
            "		\\fbox{",
            "			\\includegraphics[width=150mm]{filename.png}",
            "		}",
            "		\\caption{FigName}",
            "		\\label{FigLabel}",
            "	\\end{center}",
            "\\end{figure}",
        ],
        "description": "図を挿入"
    },
    "tabular": {
        "prefix": "tabular",
        "body": [
            "\\begin{tabular}{l}",
            "	",
            "\\end{tabular}",
        ],
        "description": ""
    },

}